<% layout('boilerplate') -%>
<div class="container">
    <div class="row row-cols-1 row-cols-md-3 g-4">
        <%for(let i=0;i<data.length;i++){%>
            <div class="col">
                
                <div class="card h-100 m-3"> <!-- Add margin class -->
                  <a href="/admin/<%=data[i].id%>"><img src=<%=data[i].images[0]%>   class="card-img-top" alt="..."></a>
                  <div class="card-body">
                    <h5 class="card-title"><%=data[i].name%></h5>
                    <p class="card-text"><b>SocialMedia  : </b><%=data[i].socialMedia%></p>
                    <form action="/admin/delete/<%=data[i].id%>?_method=DELETE" method="post">
                    <button  class="btn btn-danger">Delete</button>

                    </form>
                  </div>
                </div>
              </div>
             
        <%}%>
      
     
    
      </div>
</div>
